MODULE ProjectFilter;

REQUIRE Project;

PRIORITY MasterData;

NAMESPACE ProjectManagement;

@defineDocFilter(project, ProjectType, type, p, 'Type');
@defineDocFilter(project, Company, company, p, 'Company');
@defineDocFilter(project, Partner, partner, p, 'Partner');
@defineDocFilter(project, ProjectStatus, status, p, 'Status');
@defineDocFilter(project, Employee, manager, p, 'Manager');
