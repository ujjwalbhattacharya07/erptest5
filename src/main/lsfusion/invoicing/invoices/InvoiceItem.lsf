MODULE InvoiceItem;

REQUIRE Invoice;

NAMESPACE Invoicing;

EXTEND FORM item
    OBJECTS il = InvoiceLine
    PROPERTIES(il) READONLY number, dateTime, nameCustomer, imagedNameStatus, nameCompany, quantity, price
    PROPERTIES(il) NEWSESSION EDIT
    FILTERS item(il) = i
;

DESIGN item {
    operations {
        NEW invoices {
            caption = 'Invoices';
            MOVE BOX(il);
        }
    }
}